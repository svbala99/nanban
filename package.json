{
    "name": "nanban",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "postinstall": "patch-package",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "lint": "eslint . --fix",
        "prepare": "husky install",
        "start": "react-native start",
        "test": "jest",
        "kn": "killall node",
        "adbr": "adb reverse tcp:8081 tcp:8081",
        "tron": "adb reverse tcp:9090 tcp:9090",
        "reset-c": "yarn start -- --reset-cache",
        "node-c": "rm -rf node_modules&& yarn cache clean && yarn install",
        "setDevelopment": "ENVFILE=.env.development",
        "setProduction": "ENVFILE=.env.production",
        "pod-c": "cd ios && pod deintegrate && pod setup && pod install",
        "ios-c": "yarn node-c &&  cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && yarn ios:pod:Reset",
        "pod": "cd ios && pod install && cd ..",
        "ios:ba": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
        "ios:dev": "yarn setDevelopment  react-native run-ios --mode=Debug --scheme \"development\"",
        "ios:dev-release": "yarn ios-c && yarn setDevelopment  react-native run-ios --mode=Release --scheme \"development\"",
        "ios:prod": "yarn setProduction  react-native run-ios --mode=Debug --scheme \"production\"",
        "ios:prod-release": " yarn ios-c && yarn ios-ba && yarn setProduction  react-native run-ios --mode=Release --scheme \"production\"",
        "aos-c": "yarn node-c && cd android && ./gradlew clean",
        "aos-ba": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
        "aos-ra": "rm -rf android/app/src/main/res/drawable-hdpi &&  rm -rf android/app/src/main/res/drawable-mdpi &&  rm -rf android/app/src/main/res/drawable-xhdpi && rm -rf android/app/src/main/res/drawable-xxhdpi && rm -rf android/app/src/main/res/drawable-xxxhdpi && rm -rf android/app/src/main/res/raw",
        "aos:dev": "yarn setDevelopment  react-native run-android --mode=developmentDebug --appIdSuffix=development",
        "aos:dev-release": "yarn setDevelopment  react-native run-android --mode=developmentRelease--appIdSuffix=development",
        "aos:prod": "yarn setProduction  react-native run-android --mode=productionDebug --appIdSuffix=production",
        "aos:prod-release": "yarn setProduction  react-native run-android --mode=productionRelease --appIdSuffix=production",
        "aos:dd-apk": "yarn setDevelopment &&  yarn aos-c && yarn aos-ba && yarn aos-ra cd android && ./gradlew assembleDevelopmentDebug && killall -9 java",
        "aos:dr-apk": "yarn setDevelopment &&  yarn aos-c && yarn aos-ba && yarn aos-ra cd android && ./gradlew assembleDevelopmentRelease && killall -9 java",
        "aos:pd-apk": "yarn setProduction &&  yarn aos-c && yarn aos-ba && yarn aos-ra cd android && ./gradlew assembleProductionDebug && killall -9 java",
        "aos:pr-apk": "yarn setProduction &&  yarn aos-c && yarn aos-ba && yarn aos-ra cd android && ./gradlew assembleProductionRelease && killall -9 java",
        "aos:pr-bundle": "yarn setProduction &&  yarn clean && yarn aos-ba && yarn aos-ra && cd android && ./gradlew bundleProductionRelease && killall -9 java",
        "open-apk": "open ./android/app/build/outputs/apk/",
        "open-bundle": "open ./android/app/build/outputs/bundle/productionRelease",
        "aos:dev-apk": "yarn aos:dd-apk && yarn open-apk",
        "aos:dev:release-apk": "yarn aos:dr-apk && yarn open-apk",
        "aos:prod-apk": "yarn aos:pd-apk && yarn open-apk",
        "aos:prod:release-apk": "yarn aos:pr-apk && yarn open-apk",
        "aos:bundle": "yarn aos:pr-bundle && yarn open-bundle"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@react-native-firebase/app": "^18.4.0",
        "@react-native-firebase/auth": "^18.4.0",
        "@react-navigation/bottom-tabs": "^6.5.8",
        "@react-navigation/native": "^6.1.7",
        "@react-navigation/stack": "^6.3.17",
        "patch-package": "^8.0.0",
        "react": "^18.2.0",
        "react-native": "^0.72.3",
        "react-native-config": "^1.5.1",
        "react-native-gesture-handler": "^2.12.1",
        "react-native-safe-area-context": "^4.7.1",
        "react-native-screens": "^3.24.0",
        "react-native-vector-icons": "^10.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.22.10",
        "@babel/eslint-parser": "^7.22.10",
        "@babel/preset-env": "^7.22.9",
        "@babel/runtime": "^7.22.6",
        "@commitlint/cli": "^17.7.0",
        "@commitlint/config-conventional": "^17.7.0",
        "@jest/globals": "^29.6.2",
        "@react-native/eslint-config": "^0.72.2",
        "@react-native/metro-config": "^0.72.9",
        "@tsconfig/react-native": "^3.0.2",
        "@types/react": "^18.2.18",
        "@types/react-native-vector-icons": "^6.4.13",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "babel-jest": "^29.6.2",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.46.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-standard-with-typescript": "^37.0.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.33.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.0.0",
        "husky": "^8.0.3",
        "jest": "^29.6.2",
        "lint-staged": "^13.2.3",
        "metro-react-native-babel-preset": "^0.77.0",
        "prettier": "^3.0.1",
        "react-test-renderer": "^18.2.0",
        "typescript": "*",
        "yarn-upgrade-all": "^0.7.2"
    },
    "engines": {
        "node": ">=16"
    }
}
